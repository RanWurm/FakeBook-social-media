<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.projectpart2"
        tools:targetApi="31">
        <activity
            android:name="com.example.projectpart2.EditPostActivity"
            android:exported="false"
            android:label="EditPostActivity"
            android:theme="@style/Theme.projectpart2"/>
        <activity
            android:name="com.example.projectpart2.newPostActivity"
            android:exported="false"
            android:label="newPostActivity"
            android:theme="@style/Theme.projectpart2"/>
        <activity
            android:name="com.example.projectpart2.FeedActivity"
            android:exported="false"
            android:label="FeedActivity"
            android:theme="@style/Theme.projectpart2"/>
        <activity
            android:name="com.example.projectpart2.RegisterActivity"
            android:exported="false"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.projectpart2" />
        <activity
            android:name="com.example.projectpart2.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.projectpart2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>